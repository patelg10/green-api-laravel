{
    "name": "patelg10/green-api-laravel",
    "description": "A clean Laravel wrapper for the Green API WhatsApp service.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jiten Patel",
            "email": "patelg10@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "PatelG10\\GreenApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PatelG10\\GreenApi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PatelG10\\GreenApi\\GreenApiServiceProvider"
            ],
            "aliases": {
                "GreenApi": "PatelG10\\GreenApi\\Facades\\GreenApi"
            }
        }
    }
}